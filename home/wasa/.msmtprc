account private
host smtp.gmail.com
port 587
protocol smtp
auth on
from hurrus.durrus@gmail.com
user hurrus.durrus@gmail.com
passwordeval sed -n -e 's,^machine smtp.gmail.com login hurrus.durrus@gmail.com password \(.*\),\1,p' < ~/.netrc
tls on
tls_nocertcheck

