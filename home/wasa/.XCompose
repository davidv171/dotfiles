# shorthand
<Multi_key> <l> <o> <o> <k>           : "ಠ_ಠ"

# silly signs
<Multi_key> <c> <c> <c> <p>           : "☭"
<Multi_key> <less> <3>                : "♥"
<Multi_key> <l> <o> <v> <e>           : "♥"

# math
<Multi_key> <plus> <minus>            : "±"
<Multi_key> <minus> <plus>            : "±"
<Multi_key> <equal> <slash>           : "≠"
<Multi_key> <slash> <equal>           : "≠"
<Multi_key> <equal> <asciitilde>      : "≈"
<Multi_key> <asciitilde> <equal>      : "≈"
<Multi_key> <asciitilde> <asciitilde> : "≈"
<Multi_key> <8>                       : "∞"
<Multi_key> <x> <x>                   : "×"
<Multi_key> <comma> <minus>           : "¬"
<Multi_key> <minus> <comma>           : "¬"
<Multi_key> <p> <i>                   : "π"

# copyright
<Multi_key> <c> <o>                   : "©"
<Multi_key> <r> <o>                   : "®"
<Multi_key> <t> <m>                   : "™"

# currency
<Multi_key> <e> <equal>               : "€"
<Multi_key> <equal> <e>               : "€"
<Multi_key> <l> <minus>               : "£"
<Multi_key> <minus> <l>               : "£"
<Multi_key> <l> <equal>               : "¥"
<Multi_key> <equal> <l>               : "¥"

# quotes
<Multi_key> <greater> <space>         : "›"
<Multi_key> <less> <space>            : "‹"
<Multi_key> <less> <less>             : "«"
<Multi_key> <greater> <greater>       : "»"

<Multi_key> <grave> <space>           : "‘"
<Multi_key> <apostrophe> <space>      : "’"
<Multi_key> <comma> <space>           : "‚"
<Multi_key> <grave> <grave>           : "“"
<Multi_key> <apostrophe> <apostrophe> : "”"
<Multi_key> <comma> <comma>           : "„"
<Multi_key> <apostrophe> <minus>      : "｢"
<Multi_key> <minus> <apostrophe>      : "｣"

# punctuation
<Multi_key> <period> <period>         : "…"
<Multi_key> <exclam> <exclam>         : "¡"
<Multi_key> <question> <question>     : "¿"
<Multi_key> <exclam> <question>       : "‽"
<Multi_key> <question> <exclam>       : "⸘"

# spaces
# TODO

# other typographical necessities
<Multi_key> <e> <n>                   : "–"
<Multi_key> <e> <m>                   : "—"
<Multi_key> <s> <o>                   : "§"
<Multi_key> <o> <s>                   : "§"
<Multi_key> <n> <o>                   : "№"
<Multi_key> <o> <o>                   : "°"
<Multi_key> <bracketleft> <space>     : "␣"
<Multi_key> <bracketright> <space>    : "␣"

# compound letters
<Multi_key> <s> <s>                   : "ß"
<Multi_key> <S> <S>                   : "ẞ"

